{
  "name": "wix-hubspot-integration",
  "version": "1.0.0",
  "description": "Wix <-> HubSpot bi-directional contact sync & form/lead capture (Self-Hosted App)",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "tunnel": "node scripts/tunnel.mjs",
    "dev:tunnel": "concurrently \"npm run server:dev\" \"npm run client:dev\" \"npm run tunnel\"",
    "server:dev": "ts-node-dev --respawn --transpile-only --project tsconfig.server.json src/server/index.ts",
    "client:dev": "vite --config vite.config.ts",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "vite build --config vite.config.ts",
    "build": "npm run build:server && npm run build:client",
    "start": "node dist/server/index.js",
    "test": "jest --coverage --passWithNoTests",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "wix",
    "hubspot",
    "integration",
    "crm",
    "sync",
    "self-hosted"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@wix/astro": "^2.29.0",
    "@wix/contacts": "^1.0.27",
    "@wix/dashboard": "^1.3.43",
    "@wix/design-system": "^1.257.0",
    "@wix/forms": "^1.0.404",
    "@wix/sdk": "^1.21.3",
    "@wix/wix-ui-icons-common": "^3.139.0",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "localtunnel": "^2.0.2",
    "mongoose": "^8.4.0",
    "node-cache": "^5.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.3.1",
    "@wix/cli": "^1.1.163",
    "@wix/cli-app": "^1.1.163",
    "cloudflared": "^0.7.1",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.1.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.2",
    "vite": "^5.3.1"
  }
}
